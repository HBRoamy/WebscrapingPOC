@model List<CarViewModel>;

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
<div class="text-center">
    <h1 class="display-4">Webscraping POC</h1>
    <div style="overflow:auto;">
    <table border="1" class="table">
        <thead>
            <tr>
                <th>Model</th>
                <th>Make</th>
                <th>Odometer</th>
                <th>FuelEconomy</th>
                <th>Engine</th>
                <th>Transmission</th>
                <th>ExteriorColor</th>
                <th>StockNumber</th>
                <th>AskingPrice</th>
                <th>ChromeId</th>
                <th>BodyStyle</th>
                <th>InternetPrice</th>
                <th>RetailValue</th>
                <th>NewOrUsed</th>
                <th>ModelCode</th>
                <th>VIN</th>
                <th>UUID</th>
                <th>Trim</th>
                <th>FuelType</th>
                <th>EngineSize</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Model</td>
                    <td>@item.Make</td>
                    <td>@item.Odometer</td>
                    <td>@item.FuelEconomy</td>
                    <td>@item.Engine</td>
                    <td>@item.Transmission</td>
                    <td>@item.ExteriorColor</td>
                    <td>@item.StockNumber</td>
                    <td>@item.AskingPrice</td>
                    <td>@item.ChromeId</td>
                    <td>@item.BodyStyle</td>
                    <td>@item.InternetPrice</td>
                    <td>@item.RetailValue</td>
                    <td>@item.NewOrUsed</td>
                    <td>@item.ModelCode</td>
                    <td>@item.VIN</td>
                    <td>@item.UUID</td>
                    <td>@item.Trim</td>
                    <td>@item.FuelType</td>
                    <td>@item.EngineSize</td>
                </tr>
            }
        </tbody>
    </table>
    </div>
</div>
